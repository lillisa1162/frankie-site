        var formsToProtectOnPage = [];
        var currentForm;
        var currentValidationForm;

        var onSubmit = function (token) {
            if (currentValidationForm && currentValidationForm.validator
                && currentValidationForm.validator.validate()
            ) {
                currentForm.querySelector('[name="amasty_invisible_token"]').setAttribute('value', token);
                currentForm.submit();
            } else {
                grecaptcha.reset();
            }
        };

        formsToProtect = ["#form-validate"];
        formsToProtect.forEach(function (item) {
            formToProtect = $$(item)[0];
            if (formToProtect) {
                formsToProtectOnPage.push(formToProtect);
            }
        });
        for (var index in formsToProtectOnPage) {
            if (formsToProtectOnPage.hasOwnProperty(index)) {
                var formToProtectOnPage = formsToProtectOnPage[index];
                if ('form' !== formToProtectOnPage.tagName.toLowerCase()) {
                    formToProtectOnPage = formToProtectOnPage.getElementsByTagName('form');
                    if (0 < formToProtectOnPage.length) {
                        formToProtectOnPage = formToProtectOnPage[0];
                    } else {
                        continue;
                    }
                }

                var recaptchaBlock = document.createElement('div');
                recaptchaBlock.className = 'amasty_recaptcha';
                formToProtectOnPage.appendChild(recaptchaBlock);

                var tokenInput = document.createElement('input');
                tokenInput.type = 'hidden';
                tokenInput.name = 'amasty_invisible_token';
                tokenInput.value = '';
                formToProtectOnPage.appendChild(tokenInput);

                formToProtectOnPage.onsubmit = function submitProtectedForm(event) {
                    currentForm = event.target;
                    currentValidationForm = new VarienForm(currentForm.id, false);
                    recaptchaBlock = currentForm.querySelector(".amasty_recaptcha");
                    if ('' == recaptchaBlock.innerHTML) {
                        recaptcha = grecaptcha.render(recaptchaBlock, {
                            'sitekey': '6Ldf_jwUAAAAAF9AGcjGvLWvjlgd0p5ZSeLeEwGT',
                            'callback': onSubmit,
                            'size': "invisible",
                            'badge': 'bottomright'
                        });
                    }
                    grecaptcha.reset(recaptcha);
                    grecaptcha.execute(recaptcha);
                    return false;
                }
            }
        }
    </script>
    <script src="https://www.google.com/recaptcha/api.js" async defer></script><script>
    var EuroloanPriceWidget = function(el){
        if(jQuery(el).length === 0){
            return;
        }

        this.el = jQuery(el).first();
        this.initStateHidden = jQuery(el).is(':hidden');
        this.priceContainers = jQuery(this.el).find('.euroloan-price-container');
        this.planContainers = jQuery(this.el).find('.euroloan-price-plans');
        this._initWidget();
    };

    EuroloanPriceWidget.prototype._initWidget = function(){
        this._initDetailsContainer();
        this._subscribeForPriceUpdates();
        this.setPrice(jQuery(this.el).data('euroloan-price'));
    };

    EuroloanPriceWidget.prototype._subscribeForPriceUpdates = function(){
        var productId = parseInt(jQuery(this.el).data('product-id'));
        if(productId > 0){
            if(typeof EuroloanPriceWidget.optionsPriceReload[productId] === 'undefined'){
                EuroloanPriceWidget.optionsPriceReload[productId] = [];
            }
            EuroloanPriceWidget.optionsPriceReload[productId].push(this);
        }
    };

    EuroloanPriceWidget.prototype.setPrice = function(price){
        this.price = parseFloat(price);
        this.euroloanPriceObject = EL.eCommercePaymentPlan(this.price);

        if(this.price > 0.01 && jQuery(this.priceContainers).length > 0
            && typeof this.euroloanPriceObject.smallestInstalmentPlan === 'object'
            && typeof this.euroloanPriceObject.smallestInstalmentPlan.InstalmentAmount !== 'undefined'
            && this.price >= EuroloanPriceWidget.minPrice && this.price <= EuroloanPriceWidget.maxPrice){

            this._reloadPrices();
            this._reloadPlans();
            this._activate();

        } else {
            this._deactivate();
        }
    };

    EuroloanPriceWidget.prototype._activate = function(){
        jQuery(this.el).addClass('active').fadeIn();
    };

    EuroloanPriceWidget.prototype._deactivate = function(){
        jQuery(this.priceContainers).each(function(){
            jQuery(this).text('');
        });

        jQuery(this.planContainers).each(function(){
            jQuery(this).html('');
        });

        jQuery(this.el).removeClass('active');

        if(this.initStateHidden){
            jQuery(this.el).fadeOut();
        }
    };

    EuroloanPriceWidget.prototype._initDetailsContainer = function(){
        var link = jQuery(this.el).find('.euroloan-price-details-link');
        var details = jQuery(this.el).find('.euroloan-price-details');
        if(jQuery(link).length > 0 && jQuery(details).length > 0){
            jQuery(link).on('click', function(e){
                e.preventDefault();
                jQuery(details).slideToggle();
            });
        }
    };

    EuroloanPriceWidget.prototype._reloadPrices = function(){
        var self = this;
        jQuery(this.priceContainers).each(function(){
            jQuery(this).text(
                EuroloanPriceWidget.smallestPaymentPlanText.replace(
                    EuroloanPriceWidget.priceRegex,
                    self.euroloanPriceObject.smallestInstalmentPlan.InstalmentAmount.toString()
                )
            );
        });
    };

    EuroloanPriceWidget.prototype._reloadPlans = function(){
        if(jQuery(this.planContainers).length > 0
            && typeof this.euroloanPriceObject.availablePaymentPlans === 'object'){
            var self = this;
            jQuery(this.planContainers).each(function(){
                jQuery(this).html('');
                var container = this;
                jQuery.each(self.euroloanPriceObject.availablePaymentPlans, function(i, plan){
                    if(typeof plan.InstalmentCount !== 'undefined' && plan.InstalmentAmount !== 'undefined'){
                        jQuery('<tr/>')
                            .append(jQuery('<td/>').text(
                                EuroloanPriceWidget.instalmentCountText.replace(
                                    EuroloanPriceWidget.countRegex,
                                    plan.InstalmentCount.toString()
                                )
                            ))
                            .append(jQuery('<td/>').text(
                                EuroloanPriceWidget.instalmentAmountText.replace(
                                    EuroloanPriceWidget.priceRegex,
                                    plan.InstalmentAmount.toString()
                                )
                            )).appendTo(container);
                    }
                });
            });
        }
    };

    EuroloanPriceWidget.smallestPaymentPlanText = "From XXX€ /month";
    EuroloanPriceWidget.instalmentCountText = "Loan YYY month";
    EuroloanPriceWidget.instalmentAmountText = "XXX€ /month";
    EuroloanPriceWidget.priceRegex = new RegExp("XXX");
    EuroloanPriceWidget.countRegex = new RegExp("YYY");
    EuroloanPriceWidget.minPrice = parseFloat("10");
    EuroloanPriceWidget.maxPrice = parseFloat("2000");
    EuroloanPriceWidget.optionsPriceReload = {};

    EuroloanPriceWidget._initOptionsPriceObserver = function(){
        if(typeof Product === 'object' && typeof Product.OptionsPrice !== 'undefined'){
            Product.OptionsPrice.prototype.formatPriceOrig = Product.OptionsPrice.prototype.formatPrice;
            Product.OptionsPrice.prototype.formatPrice = function(price){
                if(typeof EuroloanPriceWidget.optionsPriceReload[this.productId] === 'object'){
                    jQuery.each(EuroloanPriceWidget.optionsPriceReload[this.productId], function(i, el){
                        el.setPrice(price);
                    });
                }
                return this.formatPriceOrig(price);
            }
        }
    };

    EuroloanPriceWidget.initWidgets = function(els){
        EuroloanPriceWidget._initOptionsPriceObserver();

        if(typeof EL === 'object' && typeof EL.eCommercePaymentPlan === 'function'){
            jQuery(els).each(function(){
                new EuroloanPriceWidget(this);
            });
        } else {
            jQuery.getScript("https://elfiprod.blob.core.windows.net/public/ecommercescriptv1.js", function(){
                if(typeof EL === 'object' && typeof EL.eCommercePaymentPlan === 'function'){
                    jQuery(els).each(function(){
                        new EuroloanPriceWidget(this);
                    });
                }
            });
        }
    };

    jQuery(document).on('ready', function(){
        EuroloanPriceWidget.initWidgets(jQuery('.euroloan-price-widget'));
    });

</script><input type="hidden" id="subprouctInfo" value='[]' />
    </div>
</div>
