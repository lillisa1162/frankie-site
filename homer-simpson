console.log(typeof jQuery('#subprouctInfo').val());
jQuery(document).ready(function(){
	if(jQuery('#is_shown_subqty').val() && (isEmpty(JSON.parse(jQuery('#subprouctInfo').val())))){
	    jQuery('.stock-status').show();
